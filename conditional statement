#if statement#
a<-33
b <-200
if(b>a){
  print("b is greater than a")

}


#else if #
a<-33
b <-200
if(b>a){
  print("b is greater than a")
  
}else if (a==b){
  print("a and b are equal")
}
if else
a <-200 
b<-33
a<-33
b <-200
if(b>a){
  print("b is greater than a")
  
}else if (a==b){
  print("a and b are equal")
}else{
  print("a is greater than b")
}
